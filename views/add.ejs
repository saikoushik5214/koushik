<% layout("/Layouts/boilerplate") %>
<body>
    
    <h2>Create a New Listings</h2>
    <div class="md-center">

        <form action="/listings" method="post">
           <div>
            <label for="title" class="form-label">Title</label>
            <input type="text" class="form-control" name="title" id="title" placeholder="Enter your title"><br><br>
           </div>
           <div>
            <label for="description" class="form-label">Description</label>       
            <textarea name="description" class="form-control" id="description" placeholder="Enter your description"></textarea><br><br>
           </div>
           <div>
            <label for="image" class="form-label">Image (Paste your URL)</label>
            <input type="text" class="form-control" name="image" placeholder="Paste Your img link" id="image"><br><br>
           </div>
            <div>
                <label for="price" class="form-label">Price</label> 
                <input type="number" class="form-control" name="price" id="price" placeholder="Enter your Price"><br><br>
            </div>
            <div>
                 <label for="country" class="form-label" >Country</label>
                <input class="form-control" type="text"id = "country" name="country" placeholder="Country"><br><br>
            </div>
            <div>
                <label for="Location" class="form-label" >Location</label>
                <input class="form-control" type="text" id="Location" name="location" placeholder="Location"><br><br>
            </div>
    
             <button>Submit</button>
        </form>
    </div>
</body>
